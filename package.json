{
  "name": "react-firebase-ssr-boilerplate",
  "version": "1.0.0",
  "description": "React app with server side rendering, code and styles splitting, i18n",
  "main": "./bin/server",
  "scripts": {
    "/SITE": "-----------------------------------------------------------------",
    "clean": "rimraf ./functions/site-build",
    "site:i18n": "cross-env ENTRY=site node ./bin/i18n",
    "site:build": "npm run clean && npm run site:build:client && npm run site:build:server && npm run site:i18n && npm run site:compileServer",
    "site:build:client": "cross-env CONFIG=site.build.client node ./bin/compile",
    "site:build:server": "cross-env CONFIG=site.build.server node ./bin/compile",
    "site:server": "cross-env CONFIG=site.client.server node ./bin/server",
    "site:start": "npm run site:build && npm run site:server",
    "site:dev": "cross-env CONFIG=site.client.dev nodemon",
    "site:compileServer": "cross-env CONFIG=site.client.server node ./bin/compileServer",
    "////////": "-----------------------------------------------------------------"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mike-diamond/react-firebase-ssr-boilerplate.git"
  },
  "author": "Mike Diamond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mike-diamond/react-firebase-ssr-boilerplate/issues"
  },
  "homepage": "https://github.com/mike-diamond/react-firebase-ssr-boilerplate#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "compose-middleware": "^5.0.0",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.5.0",
    "eases": "^1.0.8",
    "express": "^4.16.3",
    "express-device": "^0.4.2",
    "firebase": "^5.5.3",
    "firebase-admin": "^6.0.0",
    "firebase-connect": "^1.1.4",
    "js-cookie": "^2.2.0",
    "react-css-modules": "^4.7.7",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-infinite-feed": "^1.0.11",
    "react-input-mask": "^2.0.4",
    "react-intl": "^2.7.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-responsive": "^5.0.0",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redaction": "^4.3.0",
